    "watch": "webpack -w --config webpack.dev.js"
  },
  "author": "Joe Martella",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.4",
    "@types/chrome": "^0.0.60",
    "@types/jest": "^26.0.4",
    "@types/material-ui": "^0.21.7",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/testing-library__jest-dom": "^5.9.1",
    "@types/testing-library__react": "^10.2.0",
    "css-loader": "^3.4.2",
    "jest": "^26.1.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "ts-jest": "^26.1.1",
    "ts-loader": "^4.0.1",
    "typescript": "^3.8.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
